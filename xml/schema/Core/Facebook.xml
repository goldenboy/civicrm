<?xml version="1.0" encoding="iso-8859-1" ?>

<table>
  <base>CRM/Core</base>
  <class>Facebook</class>
  <name>civicrm_facebook</name>
  <comment>facebook linking with civicrm.</comment>
  <add>2.0</add>
  <field>
      <name>id</name>
      <type>int unsigned</type>
      <required>true</required>
      <comment>Unique ID</comment>
      <add>2.0</add>
  </field>
  <primaryKey>
       <name>id</name>
       <autoincrement>true</autoincrement>
  </primaryKey>
  <field>
      <name>contact_id</name>
      <type>int unsigned</type>
      <comment>FK to Contact ID</comment>
      <add>2.0</add>
  </field>
  <foreignKey>
       <name>contact_id</name>
       <table>civicrm_contact</table>
       <key>id</key>
       <add>2.0</add>
       <onDelete>CASCADE</onDelete>
  </foreignKey>
  <field>
      <name>user_id</name>
      <type>int unsigned</type>
      <comment>facebook user id</comment>
      <add>2.0</add>
  </field>
  <field>
       <name>session_key</name>
       <title>Session Key</title>
       <type>varchar</type>
       <length>255</length>
       <comment>permanant authenticated session key of facebook user</comment>
       <add>2.0</add>
  </field>
  <field>
       <name>gen_key</name>
       <title>Generated Key</title>
       <type>varchar</type>
       <length>16</length>
       <comment></comment>
       <add>2.0</add>
  </field>
</table>